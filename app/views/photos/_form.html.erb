<%= render "layouts/errors", :object => @photo %>

<%= form_for @photo do |f| %>
  <%= f.label :name %>
  <br>
  <%= f.text_field :name %>
  <br>
  <%= f.label :description %>
  <br>
  <%= f.text_area :description %>
  <br>
  <%= f.label :price %>
  <br>
  <%= f.number_field :price %>
  <br>
  <br>
  <%= f.submit  %>
<% end %>
